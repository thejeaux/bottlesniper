    .</span><span><span class="hljs-title function_">replace</span></span><span>(</span><span><span class="hljs-regexp">/[^\w\s]/gi</span></span><span>, </span><span><span class="hljs-string">''</span></span><span>)
    .</span><span><span class="hljs-title function_">trim</span></span><span>();
}
