  </span><span><span class="hljs-keyword">if</span></span><span> (message.</span><span><span class="hljs-property">type</span></span><span> === </span><span><span class="hljs-string">'bottleInfo'</span></span><span>) {
    </span><span><span class="hljs-title function_">findMatchingBottle</span></span><span>(message.</span><span><span class="hljs-property">title</span></span><span>, message.</span><span><span class="hljs-property">price</span></span><span>);
