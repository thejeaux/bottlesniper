</span><span><span class="hljs-keyword">async</span></span><span> </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">findMatchingBottle</span></span><span>(</span><span><span class="hljs-params">title, price</span></span><span>) {
  </span><span><span class="hljs-keyword">const</span></span><span> normalizedTitle = </span><span><span class="hljs-title function_">normalizeTitle</span></span><span>(title);
  </span><span><span class="hljs-keyword">const</span></span><span> searchUrl = </span><span><span class="hljs-string">`https://services.baxus.co/api/search/listings?from=0&amp;size=20&amp;listed=true&amp;q=<span class="hljs-subst">${<span class="hljs-built_in">encodeURIComponent</span></span></span></span><span>(normalizedTitle)}`;

  </span><span><span class="hljs-keyword">try</span></span><span> {
    </span><span><span class="hljs-keyword">const</span></span><span> res = </span><span><span class="hljs-keyword">await</span></span><span> </span><span><span class="hljs-title function_">fetch</span></span><span>(searchUrl);
    </span><span><span class="hljs-keyword">const</span></span><span> data = </span><span><span class="hljs-keyword">await</span></span><span> res.</span><span><span class="hljs-title function_">json</span></span><span>();
    </span><span><span class="hljs-keyword">if</span></span><span> (data?.</span><span><span class="hljs-property">items</span></span><span>?.</span><span><span class="hljs-property">length</span></span><span> &gt; </span><span><span class="hljs-number">0</span></span><span>) {
      </span><span><span class="hljs-keyword">const</span></span><span> match = </span><span><span class="hljs-title function_">bestFuzzyMatch</span></span><span>(normalizedTitle, data.</span><span><span class="hljs-property">items</span></span><span>);
      </span><span><span class="hljs-keyword">if</span></span><span> (match) {
        </span><span><span class="hljs-keyword">const</span></span><span> savings = </span><span><span class="hljs-built_in">parseFloat</span></span><span>(price) - </span><span><span class="hljs-built_in">parseFloat</span></span><span>(match.</span><span><span class="hljs-property">price</span></span><span>);
        </span><span><span class="hljs-keyword">if</span></span><span> (savings &gt; </span><span><span class="hljs-number">0</span></span><span>) {
          chrome.</span><span><span class="hljs-property">storage</span></span><span>.</span><span><span class="hljs-property">local</span></span><span>.</span><span><span class="hljs-title function_">set</span></span><span>({ </span><span><span class="hljs-attr">comparisonResult</span></span><span>: { match, savings } });
        }
      }
    }
  } </span><span><span class="hljs-keyword">catch</span></span><span> (err) {
