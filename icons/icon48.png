</span><span><span class="hljs-keyword">import</span></span><span> { normalizeBottleName } </span><span><span class="hljs-keyword">from</span></span><span> </span><span><span class="hljs-string">'./normalizeBottleName.js'</span></span><span>;

</span><span><span class="hljs-keyword">export</span></span><span> </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">bestFuzzyMatch</span></span><span>(</span><span><span class="hljs-params">scrapedName, listings</span></span><span>) {
